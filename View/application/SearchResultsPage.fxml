<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SearchResultsController">
   <children>
      <Pane prefHeight="311.0" prefWidth="480.0">
         <children>
            <ListView fx:id="resultsList" layoutX="14.0" layoutY="55.0" onMouseClicked="#previewImage" prefHeight="201.0" prefWidth="225.0" />
            <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
            <Label layoutX="88.0" layoutY="19.0" text="Search Results" />
            <Button fx:id="createAlbumBtn" layoutX="133.0" layoutY="268.0" mnemonicParsing="false" onAction="#openCreateAlbumPage" text="Create Album" />
            <ImageView fx:id="displayImage" fitHeight="194.0" fitWidth="217.0" layoutX="249.0" layoutY="57.0" pickOnBounds="true" preserveRatio="true" />
            <RadioButton layoutX="14.0" layoutY="272.0" mnemonicParsing="false" text="Select All" />
         </children>
      </Pane>
   </children>
</AnchorPane>
