<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SlideshowController">
   <children>
      <Pane prefHeight="424.0" prefWidth="527.0">
         <children>
            <ImageView fx:id="imageDisplay" fitHeight="354.0" fitWidth="510.0" layoutX="9.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="moveForward" layoutX="285.0" layoutY="384.0" mnemonicParsing="false" text="&gt;" onAction="#moveForward"/>
            <Button fx:id="moveBack" layoutX="227.0" layoutY="384.0" mnemonicParsing="false" text="&lt;" onAction="#moveBack" />
            <Button fx:id="backBtn" layoutX="14.0" layoutY="384.0" mnemonicParsing="false" text="Back" onAction="#goBack"/>
         </children></Pane>
   </children>
</AnchorPane>
